class Solution {
public:
    int numPairsDivisibleBy60(vector<int>& time) {
        vector<int> cnt(60, 0);
        int res = 0;
        for (int t : time) {
            int r = t % 60;
            int need = (60 - r) % 60;
            res += cnt[need];
            cnt[r]++;
        }
        return res;
    }
};
