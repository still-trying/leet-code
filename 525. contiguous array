class Solution {
public:
    int findMaxLength(vector<int>& nums) {


unordered_map<int, int> sumToIndex{{0, -1}};
        int maxLength = 0, cumulativeSum = 0;
        for (int i = 0; i < nums.size(); ++i) {
            cumulativeSum += (nums[i] == 1) ? 1 : -1;
            if (sumToIndex.count(cumulativeSum)) {
                int currentLength = i - sumToIndex[cumulativeSum];
                maxLength = max(maxLength, currentLength);
            } else {
                sumToIndex[cumulativeSum] = i;
            }
        }
        return maxLength;
    }
        
    
};
